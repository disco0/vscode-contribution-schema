{
    "name": "vscode-contribution-schema",
    "version": "1.0.1",
    "description": "Utility typings for working with Visual Studio Code extension contributions in TypeScript",
    "keywords": [
        "vscode",
        "typescript",
        "declaration",
        "interface",
        "contribution",
        "configuration",
        "schema",
        "json"
    ],
    "types": "./types/index.d.ts",
    "type": "commonjs",
    "module": "vscode-contribution-schema",
    "repository": {
        "url": "https://github.com/disco0/vscode-contribution-schema"
    },
    "files": [
        "types",
        "README.md",
        "media/demo-hover-markdownDescriptionMessage.png"
    ],
    "scripts": {
        "build": "yarn compile",
        "compile": "yarn run compile:types",
        "watch": "yarn run compile:types:watch",
        "compile:types": "tsc --pretty --project ./tsconfig.types.outFile.json",
        "compile:types:watch": "yarn run compile:types --watch",
        "compile:watch": "yarn run compile:watch"
    },
    "author": "disco0",
    "license": "ISC",
    "devDependencies": {},
    "dependencies": {}
}
